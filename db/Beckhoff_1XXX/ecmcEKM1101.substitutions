# accelerometer inputs
file "ecmc_analogInput-chX.template"{
  pattern { KEY,   CH_ID, ESLO,   EGU,  PREC,  sourceName   }
          { "Acc", X,     0.001,  "g",  3,    "accelerometerActual"}
          { "Acc", Y,     0.001,  "g",  3,    "accelerometerActual"}
          { "Acc", Z,     0.001,  "g",  3,    "accelerometerActual"}
}

