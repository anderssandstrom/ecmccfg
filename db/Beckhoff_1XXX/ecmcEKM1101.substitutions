# Voltages inputs
file "ecmc_analogInput-chX.template"{
  pattern { KEY,    CH_ID,    ESLO,   EGU,  PREC,  sourceName     }
          { "Volt", "UsIn",   1,      "V",  3,    "voltageActual"}
          { "Volt", "UpIn",   1,      "V",  3,    "voltageActual"}
          { "Volt", "UpOut",  1,      "V",  3,    "voltageActual"}          
}

# Voltages statuses
file "ecmc_status-chX.template"{
  pattern { KEY,    CH_ID,   sourceName     }
          { "Volt", "UsIn",  "voltageStatus"}
          { "Volt", "UpIn",  "voltageStatus"}
          { "Volt", "UpOut", "voltageStatus"}          
}
